{
  "name": "twitch-chat-mask",
  "displayName": "Twitch Chat Mask",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "okou",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo build --zip",
    "format": "prettier -w 'src/**/*.{ts,tsx}",
    "test": "plasmo test"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.8.0",
    "plasmo": "0.83.0",
    "preact": "^10.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chakra-ui/react": "^2.8.1",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@types/chrome": "0.0.245",
    "@types/node": "20.5.9",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "framer-motion": "^10.16.4",
    "prettier": "3.0.3",
    "react-icons": "^4.11.0",
    "typescript": "5.2.2"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime"
  },
  "manifest": {
    "host_permissions": [
      "https://*.twitch.tv/*"
    ]
  }
}
